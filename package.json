{
  "name": "angular-shared-components",
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$"
      }
    }
  },
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test-build-tc": "npm install --loglevel=error && ng build",
    "test-lint-tc": "eslint . --ext .ts",
    "test-unit-tc": "jest --coverage --no-colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.4",
    "@angular/cdk": "^12.1.1",
    "@angular/common": "^12.0.4",
    "@angular/compiler": "^12.0.4",
    "@angular/core": "^12.0.4",
    "@angular/elements": "^12.0.4",
    "@angular/forms": "^12.0.4",
    "@angular/material": "^12.1.1",
    "@angular/material-moment-adapter": "^12.0.4",
    "@angular/platform-browser": "^12.0.4",
    "@angular/platform-browser-dynamic": "^12.0.4",
    "@angular/router": "^12.0.4",
    "export-to-csv": "^0.2.1",
    "mat-table-exporter": "^10.2.3",
    "moment": "^2.29.1",
    "ng-packagr": "^12.1.2",
    "rxjs": "~6.6.0",
    "ts-jest": "^27.0.4",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.3",
    "@angular-eslint/builder": "12.3.1",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/schematics": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "^12.0.5",
    "@angular/compiler-cli": "^12.0.4",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.4",
    "jest-preset-angular": "^9.0.4",
    "prettier": "^2.3.1",
    "prettier-eslint": "^12.0.0",
    "typescript": "~4.2.3"
  }
}
