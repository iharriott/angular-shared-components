{
  "name": "angular-shared-components",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:shared-nav": "ng serve --project=shared-nav",
    "start:shared-form": "ng serve --project=shared-form",
    "start:shared-filter": "ng serve --project=shared-filter",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test-build-tc": "npm install --loglevel=error && ng build",
    "test-lint-tc": "eslint . --ext .ts",
    "test-unit-tc": "jest --coverage --no-colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.4",
    "@angular/cdk": "^12.0.4",
    "@angular/common": "^12.0.4",
    "@angular/compiler": "^12.0.4",
    "@angular/core": "^12.0.4",
    "@angular/forms": "^12.0.4",
    "@angular/material": "^12.1.1",
    "@angular/material-moment-adapter": "^12.0.4",
    "@angular/elements": "^12.0.4",
    "@angular/platform-browser": "^12.0.4",
    "@angular/platform-browser-dynamic": "^12.0.4",
    "@angular/router": "^12.0.4",
    "export-to-csv": "^0.2.1",
    "mat-table-exporter": "^10.2.3",
    "rxjs": "~6.6.0",
    "moment": "^2.29.1",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.3",
    "@angular/cli": "^12.0.5",
    "@angular/compiler-cli": "^12.0.4",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.4",
    "jest-preset-angular": "^9.0.4",
    "prettier": "^2.3.1",
    "prettier-eslint": "^12.0.0",
    "typescript": "~4.2.3"
  }
}
